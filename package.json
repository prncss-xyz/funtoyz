		"fix:diff": "lint-staged --diff 'HEAD^ HEAD' --allow-empty",
		"lint:all": "prettier --list-different . & eslint",
		"fix:all": "prettier --write .; eslint --fix",
		"prepare": "husky",
		"commitlint": "commitlint --edit"
