{
	"name": "funtoyz",
	"homepage": "https://github.com/prncss-xyz/funtoyz",
	"version": "0.0.0",
	"description": "",
	"scripts": {
		"update": "pnpm update --latest --recursive",
		"build": "turbo run build",
		"check": "turbo run check",
		"check:knip": "knip",
		"check:tests": "vitest run --coverage",
		"dev:tests": "vitest",
