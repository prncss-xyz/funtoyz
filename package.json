	"type": "module",
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@cspell/eslint-plugin": "^9.2.1",
		"@eslint/js": "^9.35.0",
		"@funtoyz/config": "workspace:*",
		"@turbo/gen": "^2.5.6",
		"@types/node": "^24.4.0",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.35.0",
		"eslint-plugin-perfectionist": "^4.15.0",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"knip": "^5.63.1",
		"lint-staged": "^16.1.6",
		"prettier": "^3.6.2",
		"turbo": "^2.5.6",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"vitest": "^3.2.4"
	}
